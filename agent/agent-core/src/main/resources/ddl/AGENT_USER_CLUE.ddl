CREATE TABLE `AGENT_USER_CLUE` (
  `ID`                    INT(11)      NOT NULL AUTO_INCREMENT,
  `CREATE_DATETIME`       DATETIME     NOT NULL,
  `UPDATE_DATETIME`       DATETIME     NOT NULL,
  `DISABLED`              BIT(1)       NOT NULL,
  `USER_ID`               BIGINT(20)   NOT NULL,
  `USER_NAME`             VARCHAR(255) NOT NULL,
  `REGION_CODE`           INT(11)      NOT NULL,
  `REGION_NAME`           VARCHAR(50)  NOT NULL,
  `SCHOOL_ID`             BIGINT(20)   NOT NULL,
  `SCHOOL_NAME`           VARCHAR(255) NOT NULL,
  `SCHOOL_ADDRESS`        VARCHAR(255) NOT NULL,
  `KEY_CONTACT_NAME`      VARCHAR(255) NOT NULL,
  `KEY_CONTACT_PHONE`     VARCHAR(200) NOT NULL,
  `TOTAL_STUDENT_COUNT`   INT(11)      NOT NULL,
  `HIGH_LEVEL_COUNT`      INT(11)      NOT NULL,
  `LOW_LEVEL_COUNT`       INT(11)      NOT NULL,
  `ENGLISH_TEACHER_COUNT` INT(11)      NOT NULL,
  `MATH_TEACHER_COUNT`    INT(11)      NOT NULL,
  `CHINESE_TEACHER_COUNT` INT(11)      NOT NULL,
  `PROVIDE_TYPE`          SMALLINT(6)  NOT NULL,
  `PROVIDE_PHONE`         VARCHAR(200) NOT NULL,
  PRIMARY KEY (`ID`),
  KEY `INDEX_SCHOOLID` (`SCHOOL_ID`) USING BTREE
)
  ENGINE = InnoDB;