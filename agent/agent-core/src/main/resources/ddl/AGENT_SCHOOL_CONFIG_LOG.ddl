CREATE TABLE `AGENT_SCHOOL_CONFIG_LOG` (
  `ID` bigint(20) NOT NULL AUTO_INCREMENT,
  `OPERATING_TIME` datetime NOT NULL COMMENT '操作时间',
  `REGION_ID` smallint(6) DEFAULT NULL COMMENT '大区ID',
  `REGION_NAME` varchar(50) DEFAULT NULL COMMENT '大区名称',
  `DEPARTMENT_ID` smallint(6) DEFAULT NULL COMMENT '部门ID',
  `DEPARTMENT_NAME` varchar(50) DEFAULT NULL COMMENT '部门名称',
  `HANDLERS` varchar(50) NOT NULL COMMENT '操作人',
  `HANDLER_ID` bigint(20) NOT NULL COMMENT '操作人ID',
  `OPERATION_TYPE` tinyint(4) NOT NULL COMMENT '类型',
  `SCHOOL_ID` bigint(20) NOT NULL COMMENT '学校ID',
  `SCHOOL_NAME` varchar(255) NOT NULL DEFAULT '' COMMENT '学校名称',
  `LEVEL` int(11) NOT NULL DEFAULT '1' COMMENT '学校阶段',
  `SOURCE_RESPONSIBLE` varchar(50) DEFAULT NULL COMMENT '原负责人',
  `SOURCE_RESPONSIBLE_ID` bigint(20) DEFAULT NULL COMMENT '原负责人ID',
  `NEW_RESPONSIBLE` varchar(50) DEFAULT NULL COMMENT '新负责人',
  `NEW_RESPONSIBLE_ID` bigint(20) DEFAULT NULL COMMENT '新负责人ID',
  `CREATE_DATETIME` datetime NOT NULL,
  `UPDATE_DATETIME` datetime NOT NULL,
  PRIMARY KEY (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;