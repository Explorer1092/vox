CREATE TABLE `AGENT_ONLINE_PAY_SHARE_DETAIL` (
  `ID`                   BIGINT(20)    NOT NULL AUTO_INCREMENT,
  `KPI_EVAL_DATE`        DATETIME      NOT NULL,
  `START_TIME`           DATETIME      NULL     DEFAULT NULL,
  `END_TIME`             DATETIME      NULL     DEFAULT NULL,
  `REGION_CODE`          VARCHAR(500)  NULL     DEFAULT NULL,
  `REGION_NAME`          VARCHAR(2000) NULL     DEFAULT NULL,
  `PRODUCT_NAME`         VARCHAR(20)   NOT NULL,
  `PAY_MONTH`            VARCHAR(6)    NOT NULL,
  `TOTAL_INCOME`         FLOAT         NOT NULL,
  `CARD_PAY_AMOUNT`      FLOAT         NOT NULL,
  `REFUND_AMOUNT`        FLOAT         NOT NULL,
  `OPERATION_RATE`       FLOAT         NOT NULL,
  `SHAREABLE_AMOUNT`     FLOAT         NOT NULL,
  `PAY_USER_NUM`         INT(11)       NOT NULL,
  `MONTHLY_ACTIVE_USERS` INT(11)       NOT NULL,
  `MONTHLY_PAY_RATE`     FLOAT         NOT NULL,
  `USER_ID`              BIGINT(20)    NOT NULL,
  `USER_NAME`            VARCHAR(50)   NULL     DEFAULT NULL,
  `SHARE_AMOUNT`         FLOAT         NOT NULL,
  `CREATE_DATETIME`      DATETIME      NOT NULL,
  `UPDATE_DATETIME`      DATETIME      NOT NULL,
  `FINANCE_CHECK`        BIT(1)        NOT NULL DEFAULT b'0',
  `MANAGER_CHECK`        BIT(1)        NOT NULL DEFAULT b'0',
  PRIMARY KEY (`ID`),
  KEY `IDX_USER_SHARE` (`USER_ID`, `PAY_MONTH`, `REGION_CODE`(255))
)
  ENGINE = InnoDB;